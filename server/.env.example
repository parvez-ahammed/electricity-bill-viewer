# Project Name
PROJECT_NAME=neatify

# Server
PORT=3000
JWT_EXPIRES_IN=2M
JWT_SECRET=your_jwt_secret_key_here

# For Go App and Docker Compose
DB_USER=user1
DB_PASSWORD=password1
DB_NAME=neatify
DB_PORT=5432
DB_HOST=localhost
DB_SSLMODE=disable
DB_TIMEZONE=Asia/Shanghai

# pgAdmin
PGADMIN_DEFAULT_EMAIL=example@gmail.com
PGADMIN_DEFAULT_PASSWORD=password
PGADMIN_PORT=5050


# Project Configuration
ENABLE_LATENCY_LOGGER=false

# Frontend URL (optional - defaults to http://localhost:5173)
# Note: Local network IP addresses (192.168.x.x, 10.x.x.x, 172.16-31.x.x) are automatically allowed
FRONTEND_URL=http://localhost:5173

# DPDC Service Configuration
DPDC_CLIENT_ID=auth-ui
DPDC_CLIENT_SECRET=your_dpdc_client_secret_here
DPDC_TENANT_CODE=DPDC
DPDC_COOKIE=your_dpdc_cookie_here

# NESCO Service Configuration
NESCO_COOKIE=your_nesco_cookie_here
NESCO_CSRF_TOKEN=your_nesco_csrf_token_here

# Telegram Bot Configuration
TELEGRAM_BOT_TOKEN=your_telegram_bot_token_here
TELEGRAM_CHAT_ID=your_telegram_chat_id_here

# Electricity Credentials for Telegram Reports (JSON format)
ELECTRICITY_CREDENTIALS='[{"provider":"DPDC","username":"username1","password":"password1"},{"provider":"NESCO","username":"username2","password":"password2"}]'
